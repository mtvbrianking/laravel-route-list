{
  "name": "bmatovu/laravel-route-list",
  "description": "Laravel route list.",
  "homepage": "https://github.com/mtvbrianking/laravel-route-list",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "laravel",
    "lumen",
    "package",
    "pretty",
    "route",
    "list"
  ],
  "authors": [
    {
      "name": "Brian Matovu",
      "email": "mtvbrianking@gmail.com",
      "homepage": "http://bmatovu.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "illuminate/routing": "^5.5|^6.0|^7.0|^8.0",
    "illuminate/support": "^5.5|^6.0|^7.0|^8.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.18",
    "illuminate/container": "^5.5|^6.0|^7.0|^8.0",
    "orchestra/testbench": "^3.5|^4.0|^5.0|^6.0",
    "phpunit/phpunit": "^6.0|^7.0|^8.0|^9.0"
  },
  "autoload": {
    "psr-4": {
      "Bmatovu\\RouteList\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Bmatovu\\RouteList\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "fix-cs": "php-cs-fixer fix",
    "gen-docs": "php sami.phar update .sami.dist",
    "run-tests": "phpunit",
    "run-tests-coverage": "phpunit --coverage-html coverage"
  },

  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Bmatovu\\RouteList\\RouteListServiceProvider"
      ]
    }
  }
}
